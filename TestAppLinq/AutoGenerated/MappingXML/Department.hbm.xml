<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TestAppLinq" namespace="TestAppLinq" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Department" table="Department" lazy="true" >
    <id name="DepartmentId" column="DepartmentId">
      <generator class="identity" />
    </id>
    <property name="DepartmentName">
      <column name="DepartmentName" sql-type="varchar" not-null="false" />
    </property>

  </class>
</hibernate-mapping>